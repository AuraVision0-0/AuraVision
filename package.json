    "expo-file-system": "~19.0.17",
    "expo-haptics": "~15.0.7",
    "expo-location": "~19.0.7",
    "expo-speech": "~14.0.7",
    "expo-speech-recognition": "^3.0.0",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.2.1",
